<div class="rbc-logo"></div>
<div class="form-container">
	<div class="login-form">
		<div class="logo"></div>
		<input type="email" id="emailaddress" placeholder="Email Address"/>
		<input type="password" id="password" placeholder="Password"/>
		<button id="login">Login</button>
		<div class="remember-me">
			<input type="checkbox" id="remember-me-check"/>
			<label for="remember-me-check">Remember me</label>
		</div>
		<a id="forgot-password">Forgot Password?</a>
	</div>
	<div id="signup" class="link">Don't have an account? Sign up free</div>
	<div id="help-icon">?</div>
</div>
