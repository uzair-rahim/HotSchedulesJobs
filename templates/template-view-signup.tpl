<div class="rbc-logo"></div>
<div class="form-container">
	<div class="login-form">
		<div class="logo"></div>
		<input type="text" placeholder="First Name" id="firstname"/>
		<input type="text" placeholder="Last Name" id="lastname"/>
		<input type="text" placeholder="Email Address" id="emailaddress"/>
		<input type="password" placeholder="Create Password (8 characters min)" id="password"/>
		<input type="password" placeholder="Confirm Password" id="confirm"/>
		<input type="checkbox" id="accept"/>
		<label class="terms" for="accept">I agree to the <span>Terms and Conditions</span></label>
		<button id="signup">Sign Up</button>
	</div>
	<div id="nevermind" class="link">Never mind cancel sign up</div>
	<div id="help-icon">?</div>
</div>
